<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean class="org.example.autowiring.Addresh" name="addresh">
        <property name="addresh" value="Delhi India"/>
    </bean>

    <bean class="org.example.autowiring.Student" name="studentInfo">

        <property name="studentName" value="Abhisek Bhai"/>
        <property name="studentRollNumber" value="234344"/>
        <property name="addresh" ref="addresh"/>

    </bean>



</beans>